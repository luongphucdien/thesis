{
    "eslint.format.enable": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true,
        "source.fixAll.eslint": true,
        "source.addMissingImports": true,
    },
    "editor.wordWrap": "on",
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "json"
      ],
}